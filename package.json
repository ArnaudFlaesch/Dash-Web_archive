{
  "name": "dash-web",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "homepage": "./",
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build --prod",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage --coverageDirectory=jest-coverage --watchAll=false",
    "ci": "npm run test:coverage && run-p -r start cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "wait-on http://localhost:3000 && cypress run --headless --browser chrome",
    "cy:run:ci": "npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "marge tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "snyk": "snyk test"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.21.2",
    "bootstrap": "5.1.0",
    "chart.js": "3.5.1",
    "chartjs-adapter-date-fns": "2.0.0",
    "date-fns": "2.23.0",
    "font-awesome": "4.7.0",
    "ical": "0.8.0",
    "pg": "8.7.1",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-big-calendar": "0.35.0",
    "react-chartjs-2": "3.0.4",
    "react-custom-events": "1.0.3",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "reactstrap": "8.10.0",
    "rss-parser": "3.12.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.10",
    "@cypress/instrument-cra": "1.4.0",
    "@types/chart.js": "2.9.34",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/fb": "0.0.28",
    "@types/ical": "0.8.0",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.14",
    "@types/pg": "8.6.1",
    "@types/react": "17.0.19",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-big-calendar": "0.33.1",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/react-router-dom": "5.1.8",
    "@types/reactstrap": "8.7.2",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "cypress": "8.3.1",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.5.0",
    "cypress-wait-until": "1.7.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-react-hooks": "4.2.0",
    "istanbul-lib-coverage": "3.0.0",
    "mochawesome": "6.2.2",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "5.2.0",
    "mockdate": "3.0.5",
    "nyc": "15.1.0",
    "sass": "1.39.0",
    "snyk": "1.696.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.5",
    "tslib": "2.3.1",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.4.2",
    "wait-on": "6.0.0"
  }
}
