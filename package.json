{
  "name": "dash-web",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "homepage": "./",
  "scripts": {
    "start": "craco start",
    "start:ci": "craco -r @cypress/instrument-cra start",
    "build": "craco build --prod",
    "test": "craco test --env=jsdom --testPathIgnorePatterns=./cypress/",
    "test:coverage": "npm run test -- --coverage --coverageDirectory=jest-coverage --watchAll=false",
    "ci": "npm run test:coverage && run-p -r start:ci cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "wait-on http://localhost:3000 && cypress run --headless --browser chrome",
    "cy:run:ci": "npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "marge tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@mui/icons-material": "5.1.0",
    "@mui/lab": "5.0.0-alpha.54",
    "@mui/material": "5.1.0",
    "axios": "0.24.0",
    "chart.js": "3.6.0",
    "chartjs-adapter-date-fns": "2.0.0",
    "date-fns": "2.25.0",
    "ical": "0.8.0",
    "jwt-decode": "3.1.2",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-big-calendar": "0.38.0",
    "react-chartjs-2": "3.3.0",
    "react-custom-events": "1.0.5",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.0.1",
    "react-scripts": "4.0.3",
    "react-twitter-widgets": "1.10.0",
    "rss-parser": "3.12.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@craco/craco": "6.4.0",
    "@cypress/code-coverage": "3.9.11",
    "@cypress/instrument-cra": "1.4.0",
    "@types/chart.js": "2.9.34",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/fb": "0.0.28",
    "@types/ical": "0.8.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/pg": "8.6.1",
    "@types/react": "17.0.34",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-big-calendar": "0.36.1",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "autoprefixer": "9.8.8",
    "cypress": "8.7.0",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.5.0",
    "cypress-wait-until": "1.7.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "3.2.3",
    "istanbul-lib-coverage": "3.2.0",
    "mochawesome": "7.0.1",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "6.0.1",
    "mockdate": "3.0.5",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.3.11",
    "postcss-custom-properties": "12.0.0",
    "prettier": "2.4.1",
    "sass": "1.43.4",
    "snyk": "1.759.0",
    "snyk-to-html": "2.2.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.2.17",
    "ts-jest": "27.0.7",
    "tslib": "2.3.1",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.4.4",
    "wait-on": "6.0.0"
  }
}
